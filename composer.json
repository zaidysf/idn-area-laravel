{
    "name": "zaidysf/idn-area-laravel",
    "description": "Clean Indonesian Area Data Package for Laravel - Provinces, Regencies, Districts, Villages with curated official data",
    "keywords": [
        "indonesia",
        "area",
        "provinces",
        "regencies",
        "districts",
        "villages",
        "islands",
        "laravel",
        "laravel-10",
        "laravel-11",
        "laravel-12",
        "administrative",
        "wilayah",
        "php8",
        "modern",
        "enhanced",
        "clean",
        "curated"
    ],
    "homepage": "https://github.com/zaidysf/idn-area-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Zaid Yasyaf",
            "email": "zaid.ug@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "spatie/laravel-package-tools": "^1.13.0|^1.14.0|^1.15.0|^1.16.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0|^2.0",
        "nunomaduro/collision": "^7.0|^8.0|^9.0",
        "larastan/larastan": "^2.9|^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-arch": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.4|^3.0",
        "pestphp/pest-plugin-type-coverage": "^2.8|^3.0",
        "phpstan/extension-installer": "^1.1|^1.3|^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.0|^1.1|^1.2",
        "phpstan/phpstan-phpunit": "^1.0|^1.3|^1.4",
        "mockery/mockery": "^1.4|^1.6|^1.7",
        "fakerphp/faker": "^1.21|^1.23|^1.24"
    },
    "autoload": {
        "psr-4": {
            "zaidysf\\IdnArea\\": "src/",
            "zaidysf\\IdnArea\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zaidysf\\IdnArea\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi || true"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage --min=80",
        "test-type-coverage": "vendor/bin/pest --type-coverage --min=80",
        "format": "vendor/bin/pint",
        "format-check": "vendor/bin/pint --test",
        "ci": [
            "@composer run format-check",
            "@composer run analyse",
            "@composer run test-coverage"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "zaidysf\\IdnArea\\IdnAreaServiceProvider"
            ],
            "aliases": {
                "IdnArea": "zaidysf\\IdnArea\\Facades\\IdnArea"
            }
        },
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}